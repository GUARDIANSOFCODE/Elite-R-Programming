is_armstrong <- function(num) {
  digits <- as.numeric(unlist(strsplit(as.character(num), "")))
  sum_of_cubes <- sum(digits^length(digits))
  
  if (sum_of_cubes == num) {
    return(TRUE)
  } else {
    return(FALSE)
  }
}

# Test the function
num <- 153
print(paste(num, "is Armstrong:", is_armstrong(num)))
